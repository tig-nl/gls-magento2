{
    "name": "tig/gls-magento2",
    "description": "TIG Magento 2 GLS extension",
    "require": {
        "php": "~7.1|~7.2",
        "magento/framework": ">=101.0.0,<=101.0.9|>=102.0.0,<=102.0.2"
    },
    "type": "magento2-module",
    "license": "CC-BY-NC-ND-3.0",
    "version": "1.0.0",
    "authors": [
        {
            "name": "TIG",
            "email": "support@tig.nl",
            "homepage": "https://tig.nl"
        }
    ],
    "support": {
        "email": "support@tig.nl",
        "issues": "https://portal.tig.nl/"
    },
    "homepage": "https://tig.nl/",
    "keywords": [
        "GLS",
        "Official GLS extension",
        "Magento 2"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "TIG\\GLS\\": ""
        }
    },
    "require-dev": {
        "object-calisthenics/phpcs-calisthenics-rules": "^2.0",
        "magento/marketplace-eqp": "^1.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    }
}
